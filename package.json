{
  "name": "booklet-frontend",
  "version": "0.0.1",
  "repository": "git@github.com:long-run/booklet-frontend.git",
  "author": "shrimp1box <shrimp1box@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./src/index.html & onchange './src/**/*.{html,ts,tsx,js}' -- prettier --write {{changed}}",
    "clean": "rimraf dist & rimraf node_modules & rimraf .cache",
    "build": "yarn clean & yarn & parcel build ./src/index.html --no-source-maps"
  },
  "browserslist": [
    "last 2 versions",
    "Android >= 4.2",
    "iOS >= 10",
    "ie > 10"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@emotion/core": "^10.0.6",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "babel-plugin-emotion": "^10.0.6",
    "onchange": "^5.2.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "1.15.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
